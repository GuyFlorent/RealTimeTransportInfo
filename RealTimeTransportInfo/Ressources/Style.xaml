<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RealTimeTransportInfo.Ressources">
    <Style x:Key="Style1" TargetType="Grid">
        <Setter Property="Background" Value="DarkBlue"></Setter>
    </Style>

    <Style x:Key="Style3" TargetType="Grid">
        <Setter Property="Background" Value="White"></Setter>
    </Style>
    <Style x:Key="Style4" TargetType="Ellipse">
        <Setter Property="Fill" Value="OrangeRed"></Setter>
    </Style>

    <Style x:Key="Style2" TargetType="Label">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="FontSize" Value="20" />
    </Style>
</ResourceDictionary>